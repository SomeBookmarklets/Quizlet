javascript:(()=>{ var pairs=document.getElementsByClassName("TermText notranslate lang-en");var array=[];for(var i=0;i<pairs.length;i+=2){array.push(pairs[i].innerText+"\n"+pairs[i+1].innerText)}if(pairs.length!=0){localStorage.setItem("item",array.join("BALLZ"))}var pairs=localStorage.getItem("item").split("BALLZ");document.body.addEventListener("keydown",function(g){if(g.keyCode==17){var f=document.activeElement.parentElement.parentElement.parentElement.parentElement.parentElement.getElementsByClassName("FormattedText notranslate FormattedTextWithImage-wrapper lang-en")[0];if(!f){var f=document.getElementsByClassName("WriteTextAttribute")[0]}function e(a){return a.split("\n")[0]==f.innerText}var h=pairs.find(e).split("\n")[1];navigator.clipboard.writeText(h)}}); })();
