javascript:(()=>{ function copyStr(b){var a=document.createElement("textarea");a.value=b;a.setAttribute("readonly","");a.style={position:"absolute",left:"-9999px"};document.body.appendChild(a);a.select();document.execCommand("copy");document.body.removeChild(a)}var pairs=document.getElementsByClassName("TermText notranslate lang-en");var array=[];for(var i=0;i<pairs.length;i+=2){array.push(pairs[i].innerText+"\n"+pairs[i+1].innerText)}if(pairs.length!=0){localStorage.setItem("item",array.join("BALLZ"))}var pairs=localStorage.getItem("item").split("BALLZ");document.body.addEventListener("keydown",function(d){if(d.keyCode==17){var a=document.getElementsByClassName("WriteTextAttribute")[0];function b(e){return e.split("\n")[0]==a.innerText}var c=pairs.find(b).split("\n")[1];navigator.clipboard.writeText(c)}}); })();
